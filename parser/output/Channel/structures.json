[
  {
    "name": "Channel Structure",
    "params": [
      {
        "type": {
          "reference": false,
          "value": "snowflake",
          "array": false
        },
        "name": "id",
        "description": "the id of this channel",
        "required": false
      },
      {
        "type": {
          "reference": false,
          "value": "integer",
          "array": false
        },
        "name": "type",
        "description": "the [type of channel](#DOCS_RESOURCES_CHANNEL/channel-object-channel-types)",
        "required": false
      },
      {
        "type": {
          "reference": false,
          "value": "snowflake",
          "array": false
        },
        "name": "guild_id",
        "description": "the id of the guild (may be missing for some channel objects received over gateway guild dispatches)",
        "required": true
      },
      {
        "type": {
          "reference": false,
          "value": "integer",
          "array": false
        },
        "name": "position",
        "description": "sorting position of the channel",
        "required": true
      },
      {
        "type": {
          "reference": true,
          "value": "overwrite-object",
          "array": true
        },
        "name": "permission_overwrites",
        "description": "explicit permission overwrites for members and roles",
        "required": true
      },
      {
        "type": {
          "reference": false,
          "value": "string",
          "array": false
        },
        "name": "name",
        "description": "the name of the channel (1-100 characters)",
        "required": true
      },
      {
        "type": {
          "reference": false,
          "value": "string",
          "array": false
        },
        "name": "topic",
        "description": "the channel topic (0-1024 characters)",
        "required": true
      },
      {
        "type": {
          "reference": false,
          "value": "boolean",
          "array": false
        },
        "name": "nsfw",
        "description": "whether the channel is nsfw",
        "required": true
      },
      {
        "type": {
          "reference": false,
          "value": "snowflake",
          "array": false
        },
        "name": "last_message_id",
        "description": "the id of the last message sent in this channel (may not point to an existing or valid message)",
        "required": true
      },
      {
        "type": {
          "reference": false,
          "value": "integer",
          "array": false
        },
        "name": "bitrate",
        "description": "the bitrate (in bits) of the voice channel",
        "required": true
      },
      {
        "type": {
          "reference": false,
          "value": "integer",
          "array": false
        },
        "name": "user_limit",
        "description": "the user limit of the voice channel",
        "required": true
      },
      {
        "type": {
          "reference": false,
          "value": "integer",
          "array": false
        },
        "name": "rate_limit_per_user",
        "description": "amount of seconds a user has to wait before sending another message (0-21600); bots, as well as users with the permission `manage_messages` or `manage_channel`, are unaffected",
        "required": true
      },
      {
        "type": {
          "reference": true,
          "value": "user-object",
          "array": true
        },
        "name": "recipients",
        "description": "the recipients of the DM",
        "required": true
      },
      {
        "type": {
          "reference": false,
          "value": "string",
          "array": false
        },
        "name": "icon",
        "description": "icon hash of the group DM",
        "required": true
      },
      {
        "type": {
          "reference": false,
          "value": "snowflake",
          "array": false
        },
        "name": "owner_id",
        "description": "id of the creator of the group DM or thread",
        "required": true
      },
      {
        "type": {
          "reference": false,
          "value": "snowflake",
          "array": false
        },
        "name": "application_id",
        "description": "application id of the group DM creator if it is bot-created",
        "required": true
      },
      {
        "type": {
          "reference": false,
          "value": "snowflake",
          "array": false
        },
        "name": "parent_id",
        "description": "for guild channels: id of the parent category for a channel (each parent category can contain up to 50 channels), for threads: id of the text channel this thread was created",
        "required": true
      },
      {
        "type": {
          "reference": false,
          "value": "ISO8601 timestamp",
          "array": false
        },
        "name": "last_pin_timestamp",
        "description": "when the last pinned message was pinned. This may be `null` in events such as `GUILD_CREATE` when a message is not pinned.",
        "required": true
      },
      {
        "type": {
          "reference": false,
          "value": "string",
          "array": false
        },
        "name": "rtc_region",
        "description": "[voice region](#DOCS_RESOURCES_VOICE/voice-region-object) id for the voice channel, automatic when set to null",
        "required": true
      },
      {
        "type": {
          "reference": false,
          "value": "integer",
          "array": false
        },
        "name": "video_quality_mode",
        "description": "the camera [video quality mode](#DOCS_RESOURCES_CHANNEL/channel-object-video-quality-modes) of the voice channel, 1 when not present",
        "required": true
      },
      {
        "type": {
          "reference": false,
          "value": "integer",
          "array": false
        },
        "name": "message_count",
        "description": "an approximate count of messages in a thread, stops counting at 50",
        "required": true
      },
      {
        "type": {
          "reference": false,
          "value": "integer",
          "array": false
        },
        "name": "member_count",
        "description": "an approximate count of users in a thread, stops counting at 50",
        "required": true
      },
      {
        "type": {
          "reference": true,
          "value": "thread-metadata-object",
          "array": false
        },
        "name": "thread_metadata",
        "description": "thread-specific fields not needed by other channels",
        "required": true
      },
      {
        "type": {
          "reference": true,
          "value": "thread-member-object",
          "array": false
        },
        "name": "member",
        "description": "thread member object for the current user, if they have joined the thread, only included on certain API endpoints",
        "required": true
      },
      {
        "type": {
          "reference": false,
          "value": "integer",
          "array": false
        },
        "name": "default_auto_archive_duration",
        "description": "default duration that the clients (not the API) will use for newly created threads, in minutes, to automatically archive the thread after recent activity, can be set to: 60, 1440, 4320, 10080",
        "required": true
      },
      {
        "type": {
          "reference": false,
          "value": "string",
          "array": false
        },
        "name": "permissions",
        "description": "computed permissions for the invoking user in the channel, including overwrites, only included when part of the `resolved` data received on a slash command interaction",
        "required": true
      }
    ]
  },
  {
    "name": "Message Structure",
    "params": [
      {
        "type": {
          "reference": false,
          "value": "snowflake",
          "array": false
        },
        "name": "id",
        "description": "id of the message",
        "required": false
      },
      {
        "type": {
          "reference": false,
          "value": "snowflake",
          "array": false
        },
        "name": "channel_id",
        "description": "id of the channel the message was sent in",
        "required": false
      },
      {
        "type": {
          "reference": false,
          "value": "snowflake",
          "array": false
        },
        "name": "guild_id",
        "description": "id of the guild the message was sent in",
        "required": true
      },
      {
        "type": {
          "reference": true,
          "value": "user-object",
          "array": false
