        "required": true
      },
      {
        "type": {
          "reference": false,
          "value": "boolean",
          "array": false
        },
        "name": "pinned",
        "description": "whether this message is pinned",
        "required": false
      },
      {
        "type": {
          "reference": false,
          "value": "snowflake",
          "array": false
        },
        "name": "webhook_id",
        "description": "if the message is generated by a webhook, this is the webhook's id",
        "required": true
      },
      {
        "type": {
          "reference": false,
          "value": "integer",
          "array": false
        },
        "name": "type",
        "description": "[type of message](#DOCS_RESOURCES_CHANNEL/message-object-message-types)",
        "required": false
      },
